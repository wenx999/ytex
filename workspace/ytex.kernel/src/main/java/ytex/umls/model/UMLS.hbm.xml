<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ytex.umls.model" schema="umls">
	<!--  mrrel doesn't really have a primary key / unique index -->
	<class name="MRREL" table="MRREL">
		<composite-id>
			<key-property name="cui1"></key-property>
			<key-property name="cui2"></key-property>
			<key-property name="rel"></key-property>
			<key-property name="sab"></key-property>
		</composite-id>
		<property name="rela"/>
	</class>
	<query name="getRelationsForSABs"><![CDATA[
		select cui1, cui2
		from MRREL
		where sab in (:sabs)
		and rel in (:rel)
		and rela not in (:relaExclude)
	]]></query>
	<query name="getAllRelations"><![CDATA[
		select cui1, cui2
		from MRREL
		where rel in ('PAR') and rela in ('inverse_isa')
	]]></query>
</hibernate-mapping>