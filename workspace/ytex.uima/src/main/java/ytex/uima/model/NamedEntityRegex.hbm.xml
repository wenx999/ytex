<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ytex.uima.model">
	<class name="NamedEntityRegex" table="ref_named_entity_regex">
		<id name="namedEntityRegexID" column="named_entity_regex_id">
			<generator class="native">
				<param name="sequence">named_entity_regex_id_sequence</param>
			</generator>
		</id>
		<property name="code" column="code" />
		<property name="codingScheme" column="coding_scheme" />
		<property name="oid" column="oid" />
		<property name="regex" column="regex" />
		<property name="context" column="context" />
	</class>
</hibernate-mapping>
	
	
