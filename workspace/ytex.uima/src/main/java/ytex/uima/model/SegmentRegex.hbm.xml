<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ytex.uima.model">
	<class name="SegmentRegex" table="ref_segment_regex">
		<id name="SegmentRegexID" column="segment_regex_id">
			<generator class="native">
				<param name="sequence">segment_regex_id_sequence</param>
			</generator>
		</id>
		<property name="regex" column="regex" />
		<property name="segmentID" column="segment_id" />
		<property name="limitToRegex" column="limit_to_regex" />
	</class>
</hibernate-mapping>
	
	
