# database name same as schema
db.name=ytex
# host name of mysql server
db.host=localhost
# schema name same as database 
db.schema=ytex
# username
db.username=ytex
# password
db.password=ytex

# jdbc url for database.  Replace localhost and VACS_TEST 
# with the values of db.host and db.name repsectively
db.url=jdbc:mysql://localhost:3306/ytex

# you should not have to modify anything below
db.type=mysql
db.driver=com.mysql.jdbc.Driver
db.initStatements=
ytex.beanRefContext=classpath*:beanRefContext.xml
hibernate.dialect=org.hibernate.dialect.MySQLMyISAMDialect

# the schema where umls is installed
# if not specified, will default to schema from ytex
#umls.schema=umls
