<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model">
	<class name="InstanceGram" table="instance_gram">
		<id name="instanceGramId" column="instance_gram_id">
			<generator class="native">
				<param name="sequence">instance_gram_id_sequence</param>
			</generator>
		</id>
  		<property name="name"/>
		<property name="instanceId1" column="instance_id1" />
		<property name="instanceId2" column="instance_id2" />
		<property name="similarity" column="similarity" />
	</class>
	<query name="getInstanceGram"><![CDATA[
		from InstanceGram
		where name = :name
		and instanceId1 = :instanceId1
		and  instanceId2 =  :instanceId2
	]]></query>
	<query name="deleteInstanceGram"><![CDATA[
		delete from InstanceGram
		where name = :name
		and instanceId1 = :instanceId1
		and  instanceId2 =  :instanceId2
	]]></query>
</hibernate-mapping>