<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="NegexAnnotator">
      <import name="ytex.uima.annotators.NegexAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TokenizerAnnotator">
      <import name="tokenizer.TokenizerAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="ContextDependentTokenizerAnnotator">
      <import name="context dependent tokenizer.ContextDependentTokenizerAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DocumentInfoAnnotator">
      <import name="ytex.vacs.uima.annotators.DocumentInfoAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="ParagraphAnnotator">
      <import name="paragraph.ParagraphAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="SegmentRegexAnnotator">
      <import name="ytex.uima.annotators.SegmentRegexAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DictionaryLookupAnnotatorMSSQL">
      <import name="ytex.uima.annotators.DictionaryLookupAnnotatorMSSQL"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="SentenceAnnotator">
      <import name="sentence.SentenceAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="AdjustNounPhraseToIncludeFollowingNP">
      <import name="chunker.AdjustNounPhraseToIncludeFollowingNP"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="POSTagger">
      <import name="POS tagger.POSTagger"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="Chunker">
      <import name="chunker.Chunker"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="SizeLocationRegExAnnotator">
      <import name="size location regex.SizeLocationRegExAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="LookupWindowAnnotator">
      <import name="lookup.LookupWindowAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="NamedEntityRegexAnnotator">
      <import name="ytex.uima.annotators.NamedEntityRegexAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="SentenceNumberAnnotator">
      <import name="section.SentenceNumberAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="LvgAnnotator">
      <import name="LVG.LvgAnnotator"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>AggregateAllAnnotation</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>Patterns</name>
        <description>List of room number regular expression
					pattterns.</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ParagraphAnnotator/Patterns</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>Patterns</name>
        <value>
          <array>
            <string>:\n|\z|\n\n|\n\r\n\r</string>
          </array>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>SegmentRegexAnnotator</node>
        <node>ParagraphAnnotator</node>
        <node>SentenceAnnotator</node>
        <node>SentenceNumberAnnotator</node>
        <node>TokenizerAnnotator</node>
        <node>LvgAnnotator</node>
        <node>ContextDependentTokenizerAnnotator</node>
        <node>POSTagger</node>
        <node>Chunker</node>
        <node>AdjustNounPhraseToIncludeFollowingNP</node>
        <node>LookupWindowAnnotator</node>
        <node>DictionaryLookupAnnotatorMSSQL</node>
        <node>DocumentInfoAnnotator</node>
        <node>NamedEntityRegexAnnotator</node>
        <node>SizeLocationRegExAnnotator</node>
        <node>NegexAnnotator</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">gov.va.maveric.uima.section.type.Paragraph</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.sentence.type.Sentence</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.BaseToken</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.WordToken</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.NamedEntity</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.context.type.NEContext</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.OntologyConcept</type>
          <type allAnnotatorFeatures="true">gov.va.maveric.uima.section.type.SentenceNumber</type>
          <type allAnnotatorFeatures="true">org.ohnlp.medkat.taes.sizeLocationRegExAnnotator.type.UnitExpression</type>
          <type allAnnotatorFeatures="true">org.ohnlp.medkat.taes.sizeLocationRegExAnnotator.type.SizeExpression</type>
          <type allAnnotatorFeatures="true">org.ohnlp.medkat.taes.sizeLocationRegExAnnotator.type.NumberExpression</type>
          <type allAnnotatorFeatures="true">org.ohnlp.medkat.taes.sizeLocationRegExAnnotator.type.LymphLevelExpression</type>
          <type allAnnotatorFeatures="true">org.ohnlp.medkat.taes.sizeLocationRegExAnnotator.type.LocationExpression</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.SymbolToken</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.NumToken</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.NewlineToken</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.DocumentID</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.ContractionToken</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.NamedEntity</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.context.type.NEContext</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.OntologyConcept</type>
          <type allAnnotatorFeatures="true">org.ohnlp.medkat.taes.sizeLocationRegExAnnotator.type.NumberExpression</type>
          <type allAnnotatorFeatures="true">org.ohnlp.medkat.taes.sizeLocationRegExAnnotator.type.SizeExpression</type>
          <type allAnnotatorFeatures="true">org.ohnlp.medkat.taes.sizeLocationRegExAnnotator.type.UnitExpression</type>
          <type allAnnotatorFeatures="true">org.ohnlp.medkat.taes.sizeLocationRegExAnnotator.type.LymphLevelExpression</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.NumToken</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.WordToken</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.SymbolToken</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.core.ae.type.BaseToken</type>
          <type allAnnotatorFeatures="true">gov.va.maveric.uima.ctakes.NegatedCui</type>
          <type allAnnotatorFeatures="true">gov.va.maveric.uima.ctakes.NENegation</type>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.uima.cdt.ae.type.MeasurementAnnotation</type>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
